{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js",
      "headers": {
        "origin": "http://localhost:5173",
        "optionsSuccessStatus": "204",
        "methods": "GET,HEAD,PUT,PATCH,POST,DELETE",
        "preflightContinue": "false",
        "Access-Control-Allow-Origin": "http://localhost:5173"
      }
    }
  ]
}